{
  "name": "@rarible/ethereum-api-client",
  "version": "0.2.1-beta.3",
  "keywords": [
    "axios",
    "typescript",
    "openapi",
    "openapi-generator",
    "api_documentation"
  ],
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build",
    "typings.d.ts"
  ],
  "license": "MIT",
  "scripts": {
    "download": "rimraf ./api-docs && download $API_SPEC_PATH --out=\"./api-docs\"",
    "generate": "rimraf ./src && openapi-generator-cli generate && rimraf ./src/models && mvn -f model/pom.xml clean com.rarible.codegen:model-generator-maven-plugin:generate && move-file model/target/models ./src/models",
    "build": "rimraf ./build && tsc"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "2.3.6",
    "@rarible/types": "~0.10.0-beta.1",
    "download-cli": "1.1.1",
    "move-file-cli": "2.0.0",
    "rimraf": "^5.0.0",
    "rxjs": "^6.0.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "peerDependencies": {
    "@rarible/types": ">=0.10.0 <0.11.0"
  }
}
