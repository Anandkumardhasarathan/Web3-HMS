{
  "name": "@rarible/flow-api-client",
  "version": "0.1.0-alpha.4",
  "description": "Flow js API client to interact with Rarible protocol",
  "license": "MIT",
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "types": "build/esm/index.d.ts",
  "files": [
    "build",
    "src/typings.d.ts"
  ],
  "scripts": {
    "download": "rimraf ./api-docs && download $API_SPEC_PATH --out=\"./api-docs\"",
    "generate": "rimraf ./src && openapi-generator-cli generate && rimraf ./src/models && mvn -f model/pom.xml clean com.rarible.codegen:model-generator-maven-plugin:generate && move-file model/target/models ./src/models && rimraf model/target",
    "build": "tsc -b tsconfig-build-esm.json tsconfig-build-cjs.json",
    "clean": "rimraf ./build",
    "test": "echo 0",
    "verify": "tsc --noEmit"
  },
  "peerDependencies": {
    "@rarible/types": ">=0.10.0-0 <0.11.0"
  },
  "devDependencies": {
    "@rarible/types": "~0.10.0-alpha.46",
    "download-cli": "1.1.1",
    "move-file-cli": "2.0.0",
    "typescript": "^3.8.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "gitHead": "2a24084da022c2cfd48f3c8991e09bccca893aa2"
}
